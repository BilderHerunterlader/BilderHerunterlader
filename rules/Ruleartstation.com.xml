  <pipes>
    <pipeline mode="1" waitBeforeExecute="0" urlDecodeResult="false">
      <regexp search="&quot;image_url\\&quot;:\\&quot;(.+?)large(.+?)&quot;" replace="$1large$2" />
    </pipeline>
  </pipes>
  <pipesFailures />
