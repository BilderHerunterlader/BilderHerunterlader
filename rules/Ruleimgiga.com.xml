    <pipeline mode="1" waitBeforeExecute="0" urlDecodeResult="false">
      <regexp search="&lt;script type=&quot;text/javascript&quot; src=&quot;(imgiga/showimg\.php.+?)&quot;" replace="$URL$1" />
