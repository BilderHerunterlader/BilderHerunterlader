<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<izpack:shortcuts version="5.0"
                  xmlns:izpack="http://izpack.org/schema/shortcuts"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">
	<skipIfNotSupported/>
	<programGroup defaultName="BilderHerunterlader" location="applications"/>
	<shortcut
		name="BilderHerunterlader"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no"
		startup="no"
		target="$INSTALL_PATH\BH.jar"
		commandLine=""
		workingDirectory="$INSTALL_PATH\"
		description="BilderHerunterlader"
		iconFile="$INSTALL_PATH\BHIcon.ico">
	</shortcut>
	
	<shortcut
		name="BilderHerunterlader_Java9"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no"
		startup="no"
		target="&quot;%ProgramData%\Oracle\Java\javapath\javaw.exe&quot;"
		commandLine="--add-modules ALL-SYSTEM -jar &quot;$INSTALL_PATH\BH.jar&quot;"
		workingDirectory="$INSTALL_PATH\"
		description="BilderHerunterlader_Java9"
		iconFile="$INSTALL_PATH\BHIcon.ico">
	</shortcut>
	
	<shortcut
		name="Uninstall BilderHerunterlader"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no"
		startup="no"
		target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
		description="Uninstall BilderHerunterlader">
	</shortcut>
</izpack:shortcuts>
